                      "    super();",
                      "",
                      "    document.addEventListener('wheel', scrollHandler(this), {",
                      "        capture: false, passive: true })",
                      "        const style = document.createElement('style');",
                      "        style.innerHTML = STYLE;",
                      "        this.appendChild(style);",
                      "  }",
                      "",
