                      " *    \"phaseName\": \"sd98iop3-87v3-ni4u-9438-vjc9hu8ec80w\",",
                      " *    \"card\": {",
                      " *      \"name\": \"This is a card.\",",
                      " *      \"id\": \"gd83brj9-34n8-ce8h-fh08-4e90cni3df89\",",
