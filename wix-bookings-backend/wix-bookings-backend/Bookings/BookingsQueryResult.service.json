                  "body":
                    [ "import { resources } from \"wix-bookings-backend\";",
                      "",
                      "// ...",
                      "",
                      "bookings.queryBookings()",
                      "  .find()",
                      "  .then((results) => {",
                      "    let newQuery = results.query;",
                      "    newQuery",
                      "      .ge(\"startTime\", \"2021-01-01T15:21:41.960Z\")",
