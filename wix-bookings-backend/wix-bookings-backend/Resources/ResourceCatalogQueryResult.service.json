                  "body":
                    [ "import { resources } from \"wix-bookings-backend\";",
                      "",
                      "// ...",
                      "",
                      "resources.queryResourceCatalog()",
                      "  .find()",
                      "  .then((results) => {",
                      "    let newQuery = results.query;",
                      "    newQuery",
                      "      .eq(\"slugs.name\",\"john-doe\")",
