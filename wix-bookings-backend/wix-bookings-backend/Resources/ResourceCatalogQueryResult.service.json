                      "  *         \"807cfa46-d96a-44ed-923f-93c55867ef8e\"",
                      "  *       ],",
                      "  *       \"status\": \"UPDATED\"",
                      "  *     },",
                      "  *     \"slugs\": [",
                      "  *       {",
                      "  *         \"name\": \"jack-black\",",
                      "  *         \"createdAt\": \"2021-02-08T09:51:05.281Z\"",
                      "  *       }",
                      "  *     ]",
                      "  *   },",
                      "  *   {",
                      "  *     \"resource\": {",
                      "  *       \"_id\": \"dc19d7db-6996-494b-8d6d-943d64e1f32a\",",
                      "  *       \"name\": \"Tom Jones\",",
                      "  *       \"email\": \"tom@yoga.com\",",
                      "  *       \"phone\": \"777 6543\",",
                      "  *       \"description\": \"Yoga and Self Defence instructor.\",",
                      "  *       \"tags\": [",
