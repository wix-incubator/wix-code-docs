                      "",
                      "",
                      "export function setAttendance() {",
                      "    const bookingId = '25a285ec-fc3b-4ca3-bb05-42ec7b510c2e'",
                      "    const attendanceInfo = {",
                      "        attended: true,",
                      "        numberOfAttendees: 3",
                      "    };",
                      "  return bookings.setAttendance(bookingId, attendanceInfo)",
                      "        .then((booking) => {",
