      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-multilingual-localization-schema.LocalizationSchema.CreateOrUpdateContentSchemaResponse"
                }
              ]
            }
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "Creates a new content schema or updates an existing one.\nTo update, identify the relvant schema by its appId, entityType and scope.\nThere are no partial updates. Updating a schema will override the existing one.",
        "description": [],
        "examples": [
          {
            "title": "createOrUpdateContentSchemaByKey example",
            "body": [
              "import { localizationSchema } from 'wix-multilingual-localization-schema';",
              "  ",
              " async function createOrUpdateContentSchemaByKey(schema) {",
              "   try {",
              "     const result = await localizationSchema.createOrUpdateContentSchemaByKey(schema);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "createOrUpdateContentSchemaByKey"
    },
