  "operations": [
    {
      "name": "getAbandonedCheckout",
      "params": [
        {
          "name": "abandonedCheckoutId",
          "type": [
            {
              "nativeType": "string"
            }
          ],
          "doc": "Abandoned checkout ID.",
          "required": true
        }
      ],
      "requiredFields": [
        "abandonedCheckoutId"
      ],
      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-ecom-backend.AbandonedCheckouts.AbandonedCheckout"
                }
              ]
            }
          }
        ],
        "doc": "The requested abandoned checkout.\n"
      },
      "docs": {
        "summary": "Retrieves an abandoned checkout.",
        "description": [
          "This function is not a universal function and runs only on the backend."
        ],
        "examples": [
          {
            "title": "getAbandonedCheckout example",
            "body": [
              "import { abandonedCheckouts } from 'wix-ecom-backend';",
              "  ",
              " async function getAbandonedCheckout(abandonedCheckoutId) {",
              "   try {",
              "     const result = await abandonedCheckouts.getAbandonedCheckout(abandonedCheckoutId);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "getAbandonedCheckout",
      "isAdminMethod": true
    },
    {
      "name": "queryAbandonedCheckouts",
      "params": [],
      "requiredFields": [],
      "ret": {
        "type": [
          {
            "referenceType": "wix-ecom-backend.AbandonedCheckouts.AbandonedCheckoutsQueryBuilder"
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "Retrieves a list of abandoned checkouts, given the provided paging, filtering, and sorting. Up to 100 abandoned checkouts can be returned per request.",
        "description": [
          "For a detailed list of supported operations, see [filtering and sorting for abandoned checkout properties](https://dev.wix.com/api/rest/wix-ecommerce/abandoned-checkouts/sort-and-filter). To learn how to query marketing consents, see [API Query Language](https://dev.wix.com/api/rest/getting-started/api-query-language).\n\nThis function is not a universal function and runs only on the backend."
        ],
        "examples": [
          {
            "title": "queryAbandonedCheckouts example",
            "body": [
              "import { abandonedCheckouts } from 'wix-ecom-backend';",
              "  ",
              " async function queryAbandonedCheckouts() {",
              "   const { items } = abandonedCheckouts.queryAbandonedCheckouts().find();",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "queryAbandonedCheckouts",
      "isAdminMethod": true
    }
