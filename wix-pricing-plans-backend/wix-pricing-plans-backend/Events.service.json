                      "/* Full event object:",
                      " * {",
                      " *   \"metadata\":{",
                      " *      \"id\": \"3063a637-...-337dd4b5d411\",",
                      " *      \"entityId\": \"d0f0e608-...-a8c18420e678\",",
                      " *      \"eventTime\": \"2021-02-03T14:07:13.105207Z\",",
                      " *      \"triggeredByAnonymizeRequest\": false",
                      " *   },",
                      " *   \"data\": {",
                      " *     \"plan\": {",
                      " *       \"_id\": \"d0f0e608-...-a8c18420e678\",",
                      " *       \"name\": \"Visitor Membership\",",
                      " *       \"description\": \"Temporary membership for visitors from out-of-town\",",
                      " *       \"perks\": [ ],",
                      " *       \"pricing\": {",
