                      " *           \"cycleCount\": 3",
                      " *         },",
                      " *         \"price\": {",
                      " *           \"value\": \"20\",",
                      " *           \"currency\": \"USD\"",
                      " *         },",
                      " *         \"freeTrialDays\": 0",
                      " *       },",
                      " *       \"public\": false,",
                      " *       \"archived\": true,",
                      " *       \"hasOrders\": true,",
