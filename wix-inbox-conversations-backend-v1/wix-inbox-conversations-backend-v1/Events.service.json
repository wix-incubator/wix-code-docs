      "name": "onConversationConversationsMerged",
      "params": [
        {
          "name": "event",
          "type": [
            {
              "referenceType": "wix-inbox-conversations-backend-v1.Events.wixVeloEventsConversationConversationsMerged",
              "displayName": "ConversationConversationsMerged"
            }
          ],
          "doc": "Information about the conversations that were merged\nand metadata for the event.\n",
          "required": false
        }
      ],
      "requiredFields": [],
      "ret": {
        "type": [
          {
            "nativeType": "void"
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "An event that triggers when a conversation\nis merged, for example when a contact is merged\nor when a visitor becomes a contact.",
        "description": [
          "If messages exist in both conversations when they merge,\nthe messages are preserved in chronological order."
        ],
        "examples": [
          {
            "title": "onConversationConversationsMerged example",
            "body": [
              "",
              "  export function wixCrmInbox_onConversationConversationsMerged(event) {",
              "    const eventId = event.metadata.id",
              "    const entityId = event.data.conversation._id;",
              "  }",
              "",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": true,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "wixCrmInbox_onConversationConversationsMerged"
    }
