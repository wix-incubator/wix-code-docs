        },
        {
          "name": "allowSecretGeneration",
          "optional": true,
          "type": [
            {
              "nativeType": "boolean"
            }
          ],
          "doc": "Whether a secret can be generated for this OAuth app.\n\nDefault: `true` until a secret has been generated for the OAuth App.\n"
        },
        {
          "name": "allowedRedirectDomains",
          "optional": true,
          "type": [
            {
              "complexType": {
                "nativeType": "Array",
                "typeParams": [
                  {
                    "nativeType": "string"
                  }
                ]
              }
            }
          ],
          "doc": "List of domains to which redirection from Wix is allowed after processes other than authentication.\n\nWhen a client redirects a user to a Wix page (for example, for checkout), the client provides a URL to redirect the user back to.\nWix only redirects the user if the URL is in one of the specified domains.\n"
        },
        {
          "name": "allowedRedirectUris",
          "optional": true,
          "type": [
            {
              "complexType": {
                "nativeType": "Array",
                "typeParams": [
                  {
                    "nativeType": "string"
                  }
                ]
              }
            }
          ],
          "doc": "List of URIs to which redirection from Wix is allowed after authentication.\n\nWhen a client redirects a user to Wix for authentication, the client provides a URI to redirect the user back to after the user has been authenticated.\nWix only redirects the user if the exact URI is contained in this array.\n"
        },
        {
          "name": "description",
          "optional": true,
          "type": [
            {
              "nativeType": "string"
            }
          ],
