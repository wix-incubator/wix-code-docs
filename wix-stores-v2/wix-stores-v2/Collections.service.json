        "doc": null
      },
      "docs": {
        "summary": "Retrieves a collection with the provided ID.",
        "description": [],
        "examples": [
          {
            "title": "getCollection example",
            "body": [
              "import { collections } from 'wix-stores.v2';",
              "  ",
              " async function getCollection(id) {",
              "   try {",
              "     const result = await collections.getCollection(id);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
