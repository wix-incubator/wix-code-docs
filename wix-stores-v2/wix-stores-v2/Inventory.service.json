        "doc": null
      },
      "docs": {
        "summary": "Subtracts a set number of items from inventory.",
        "description": [
          "The `decrementInventory()` function returns a Promise that is resolved when the specified item's quantity has been updated in the inventory.\n\nThis function is not a universal function and runs only on the backend."
        ],
        "examples": [
          {
            "title": "decrementInventory example",
            "body": [
              "import { inventory } from 'wix-stores.v2';",
              "  ",
              " async function decrementInventory(decrementData, options) {",
              "   try {",
              "     const result = await inventory.decrementInventory(decrementData, options);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "syntaxName": "decrementInventory",
      "isAdminMethod": true
    },
