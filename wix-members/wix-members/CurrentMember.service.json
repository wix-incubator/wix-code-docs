                  "body":
                    [ "import { currentMember } from 'wix-members';",
                      "",
                      "// ...",
                      "",
                      "currentMember.makeProfilePublic()",
                      "  .then((updatedMember) => {",
                      "    const newPrivacyStatus = updatedMember.privacyStatus;",
                      "    console.log('Privacy status was changed to', newPrivacyStatus);",
