      "name": "bulkCreateTiers",
      "params": [
        {
          "name": "tiers",
          "type": [
            {
              "complexType": {
                "nativeType": "Array",
                "typeParams": [
                  {
                    "referenceType": "wix-loyalty-v2.Tiers.Tier"
                  }
