              "import { webMethod, Permissions } from 'wix-web-module';",
              "import { elevate } from 'wix-auth';",
              "",
              "const elevatedGetPlacementStatus = elevate(plugins.getPlacementStatus);",
              "",
              "export const getPlacementStatus = webMethod(",
