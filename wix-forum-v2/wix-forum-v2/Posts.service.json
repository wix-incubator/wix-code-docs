      "docs": {
        "summary": "Retrieves a single post by ID.",
        "description": [],
        "examples": [
          {
            "title": "getPost example",
            "body": [
              "import { posts } from 'wix-forum.v2';",
              "  ",
              " async function getPost(postId, options) {",
              "   try {",
              "     const result = await posts.getPost(postId, options);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
