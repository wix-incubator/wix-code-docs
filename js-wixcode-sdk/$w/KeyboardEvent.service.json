    [ { "name": "altKey",
        "labels": [],
        "get": true,
        "set": false,
        "type": "boolean",
        "locations":
          [ { "lineno": 38,
              "filename": "KeyboardEvent.js" } ],
        "docs":
          { "summary": "Indicates if the Option key on a Mac or Alt key on a PC was pressed.",
            "links": [],
            "examples":
              [ { "title": "Gets the keyboard event's `altKey` property when the `altKey` is pressed",
                  "body":
                    [ "$w(\"#myElement\").onKeyPress( (event) => {",
                      "  let alt = event.altKey;  // true",
                      "} );" ],
                  "extra":
                    {  } },
                { "title": "Gets all of the keyboard event's properties",
                  "body":
                    [ "$w(\"#myElement\").onKeyPress( (event) => {",
                      "  let key  = event.key;                 // \"A\"",
                      "  let shift = event.shiftKey;           // true",
                      "  let meta = event.metaKey;             // false",
                      "  let alt = event.altKey;               // false",
                      "  let ctrl = event.ctrlKey;             // false",
                      "  let value = event.target.value;       // string value of text inside \"#myElement\" before the last KeyPress event",
                      "} );",
                      "" ],
                  "extra":
                    { "description": "This example gets the properties of a keyboard event fired when the\n `Shift` and `a` keys are pressed." } } ],
            "extra":
              {  } },
