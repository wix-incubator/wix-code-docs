    [ { "name": "altKey",
        "labels": [],
        "get": true,
        "set": false,
        "type": "boolean",
        "locations":
          [ { "lineno": 53,
              "filename": "KeyboardEvent.es6" } ],
        "docs":
          { "summary": "Indicates if the Option key on a Mac or Alt key on a PC was pressed.",
            "links": [],
            "examples":
              [ { "title": "Get the keyboard event's `altKey` property",
                  "body":
                    [ "$w(\"#myElement\").onKeyPress( (event) => {",
                      "  let alt = event.altKey;  // false",
                      "} );" ],
                  "extra":
                    {  } },
                { "title": "Get all of the keyboard event's properties",
                  "body":
                    [ "$w(\"#myElement\").onKeyPress( (event) => {",
                      "  let key  = event.key;                 // \"A\"",
                      "  let shift = event.shiftKey;           // true",
                      "  let meta = event.metaKey;             // false",
                      "  let alt = event.altKey;               // false",
                      "  let ctrl = event.ctrlKey;             // false",
                      "  let value = event.target.value;       // \"SE\"",
                      "} );",
                      "" ],
                  "extra":
                    { "description": "This example gets the properties of a keyboard event fired when the\n `Shift` and `a` keys were pressed." } } ],
            "extra":
              {  } },
