            "examples":
              [ { "title": "Get the base URL of the current page",
                  "body":
                    [ "import wixLocation from 'wix-location';",
                      "",
                      "// Premium site URL: \"https://www.domain.com/elephant?species=african-elephant\"",
                      "// Free site URL: \"https://user.wix-sites.com/zoo/elephant?species=african-elephant\"",
                      "",
                      "let baseUrl = wixLocation.baseUrl;",
                      "// Premium site: \"https://domain.com/\"",
