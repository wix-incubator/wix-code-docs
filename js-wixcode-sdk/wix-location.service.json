        "docs":
          { "summary": "Gets the path of the current page's URL.",
            "description":
              [ "The path for a regular page is after the  [`baseUrl`](wix-location/baseurl)",
                "If the page is a dynamic page or router page, the [`prefix`](wix-location/prefix) appears after the base URL, before the path.  ",
                "",
                "",
                " Premium sites:  ",
                " Path for a regular page, without a prefix: ![Premium site path](images/url_premium_path.png \"Premium site path\")  ",
                " Path for a dynamic or router page with a prefix: ![Premium site path](images/url_premium_path_with_prefix.png \"Premium site path with a prefix\")",
                "",
                " Free sites:   ",
                "  Path for a regular page, without a prefix: ![Free site path](images/url_free_path.png \"Free site path\")  ",
                "  Path for a dynamic or router page with a prefix: ![Free site path](images/url_free_path_with_prefix.png \"Free site path\")" ],
            "links": [],
            "examples":
              [ { "title": "Get the path of the current page's URL",
                  "body":
                    [ "import wixLocation from 'wix-location';",
                      "",
                      "// Premium site URL: \"https://www.domain.com/elephant?species=african-elephant#desc\"",
                      "// Free site URL: \"https://user.wix-sites.com/zoo/elephant?species=african-elephant#desc\"",
                      "",
                      "let path = wixLocation.path; // [\"elephant\"]" ],
                  "extra":
                    {  } } ],
            "extra":
              {  } },
