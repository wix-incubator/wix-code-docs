                { "title": "List the current member's orders in a table",
                  "body":
                    [ "import { orders } from 'wix-pricing-plans'; ",
                      "",
                      "// The \"columns\" constant is used to set",
                      "// the properties of the table columns.",
                      "const columns =  [",
                      "  {",
                      "    id: 'orderID',",
                      "    dataPath: '_id',",
                      "    label: 'Order',",
                      "    width: 15,",
                      "    visible: true,",
                      "    type: 'string'",
                      "    },",
                      "  {",
                      "    id: 'planName',",
                      "    dataPath: 'planName',",
                      "    label: 'Plan',",
                      "    width: 15,",
                      "    visible: true,",
                      "    type: 'string'",
                      "  },",
                      "  {",
