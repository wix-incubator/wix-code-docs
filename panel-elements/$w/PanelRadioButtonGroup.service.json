              [ { "title": "Get a text input's prefix",
                  "body":
                    [ "let radioOptions = $w(\"#myPanelRadioGroup\").options;",
                      "",
                      "let firstLabel = radioOptions[0].label;   // \"First Label\"",
                      "let firstValue = radioOptions[0].value;   // \"first_value\"" ],
                  "extra":
                    {  } },
