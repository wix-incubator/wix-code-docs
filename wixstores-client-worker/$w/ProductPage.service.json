            { "name": "title",
              "type": "string",
              "doc": "Media item title." },
            { "name": "description",
              "type": "string",
              "doc": "Media item description. Descriptions over 100 characters are truncated." },
            { "name": "type",
              "type": "string",
              "doc": "Media item type. Can be \"image\" or \"video.\"" },
            { "name": "src",
              "type": "string",
              "doc": "Media item source URL." },
            { "name": "thumbnail",
              "type": "string",
              "doc": "Thumbnail URL for videos only.",
