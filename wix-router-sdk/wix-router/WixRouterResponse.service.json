        "members":
          [ { "name": "title",
              "type": "string",
              "doc": "The page title. Written to the `<title>` tag." },
            { "name": "description",
              "type": "string",
              "doc": "**Deprecated: Use the new `metaTags` format instead.** The\n page description. Written to the `<meta name=\"description\" content=\"<value>\">` tag." },
            { "name": "keywords",
              "type": "string",
              "doc": "**Deprecated: Use the new `metaTags` format instead.** The page\n keywords. Written to the `<meta name=\"keywords\" content=\"<value>\">` tag." },
            { "name": "noIndex",
              "type": "boolean",
              "doc": "**Deprecated: Use the new `metaTags` format instead.** Indicates\n whether to add a meta tag that prevents search engines from indexing the page. Setting\n `noIndex` to `true` will add `<meta name=\"robots\" content=\"noindex\">`." },
            { "name": "metaTags",
              "type":
                { "name": "Array",
                  "typeParams":
                    [ "wix-seo.MetaTag" ] },
              "doc": "The page's SEO-related meta tags.\n\n When setting `og:image` meta tags, the `content` can be and external image URL\n or a Media Manager image URL as described [here]($w.Image.html#src).\n\n **Deprecated format:** An object with key:value pairs where the key is the meta tag name\n and the value is the content." },
            { "name": "links",
              "type":
                { "name": "Array",
                  "typeParams":
                    [ "wix-seo.Link" ] },
              "doc": "The page's SEO-related link tags which provide additional\n SEO information about the page. For example, you can set a link to a canonical\n or alternate version of the page." },
            { "name": "structuredData",
              "type":
                { "name": "Array",
                  "typeParams":
                    [ "Object" ] },
              "doc": "The page's structured data, which helps search engines \n understand more about the page and your business so they can display a richer snippet of the\n page in search results." } ],
