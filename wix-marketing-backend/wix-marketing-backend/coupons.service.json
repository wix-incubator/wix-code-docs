        "members":
          [ { "name": "name",
              "type": "string",
              "doc": "Name of coupon." },
            { "name": "code",
              "type": "string",
              "doc": "Coupon code. Must be unique for all coupons in your site. Maximum 20 characters." },
            { "name": "startTime",
              "type": "Date",
              "doc": "Start date and time of coupon." },
            { "name": "expirationTime",
              "type": "Date",
              "doc": "End date and time of coupon.",
              "optional": true },
            { "name": "usageLimit",
              "type": "number",
              "doc": "The maximum number of times a coupon can be used. Note that multiple purchases by the same customer or purchases by different customers are both counted toward usage limit.",
              "optional": true },
            { "name": "limitedToOneItem",
              "type": "boolean",
              "doc": "Indicates whether the coupon is limited to 1 discount per order. If true and a customer pays for multiple items the coupon applies to, only the lowest priced item is discounted. Coupons with a `bookings` `namespace` are always limited to one item.",
              "optional": true },
            { "name": "active",
              "type": "boolean",
              "doc": "Indicates whether the coupon is currently <a href=\"https://support.wix.com/en/article/activating-and-deactivating-coupons-in-wix-stores\" target=\"_blank\">active</a>.",
              "optional": true },
            { "name": "minimumSubtotal",
              "type": "number",
              "doc": "The coupon is only applicable when the order subtotal is over this amount.",
              "optional": true },
            { "name": "scope",
              "type": "wix-marketing-backend.coupons.CouponScope",
              "doc": "Scope of the coupon. When no scope is defined, the coupon applies to all items in all `namespaces` in the site.",
              "optional": true },
            { "name": "buyXGetY",
              "type": "wix-marketing-backend.coupons.CouponTypeBuyXGetY",
              "doc": "Coupon type: Free products when making a purchase. `buyXGetY` is an object that specifies `x` and `y` in the following scenario: if a visitor purchases x number of products, they receive y number of products for free. Currently only supported for coupons with a `stores` `namespace`.",
              "optional": true },
            { "name": "freeShipping",
              "type": "boolean",
              "doc": "Coupon type: Free shipping. If true, the coupon applies to all items in all `namespaces` in the site.",
              "optional": true },
            { "name": "moneyOffAmount",
              "type": "number",
              "doc": "Coupon type: Fixed price discount.",
              "optional": true },
            { "name": "percentOffRate",
              "type": "number",
              "doc": "Coupon type: Discount as a percentage.",
              "optional": true },
            { "name": "fixedPriceAmount",
              "type": "number",
              "doc": "Coupon type: Specific sale price. Currently only supported for coupons with a `stores` `namespace`.",
