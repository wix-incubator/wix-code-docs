              " *     \"_updatedDate\": \"2021-01-17T18:42:31.000Z\",",
              " *     \"displayName\": \"Patronus\",",
              " *     \"key\": \"custom.patronus\",",
              " *     \"dataType\": \"TEXT\",",
              " *     \"namespace\": \"custom\",",
