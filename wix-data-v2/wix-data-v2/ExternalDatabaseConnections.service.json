      "name": "createExternalDatabaseConnection",
      "params": [
        {
          "name": "options",
          "type": [
            {
              "referenceType": "wix-data-v2.ExternalDatabaseConnections.CreateExternalDatabaseConnectionOptions"
            }
          ],
          "doc": "Options for creating an external database connection.\n",
          "required": false
        }
      ],
      "requiredFields": [
        "options.externalDatabaseConnection.configuration",
        "options.externalDatabaseConnection.endpoint",
        "options.externalDatabaseConnection.name"
      ],
      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-data-v2.ExternalDatabaseConnections.CreateExternalDatabaseConnectionResponse"
                }
              ]
            }
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "Creates a new external database connection.",
        "description": [
          "The `options.externalDatabaseConnection` parameter must include a name, base URL, and configuration details for the external database.\nIf any of these are missing, the external database connection isn't created."
        ],
        "examples": [
          {
            "title": "createExternalDatabaseConnection example",
            "body": [
              "import { externalDatabaseConnections } from 'wix-data.v2';",
              "  ",
              " async function createExternalDatabaseConnection(options) {",
              "   try {",
              "     const result = await externalDatabaseConnections.createExternalDatabaseConnection(options);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
