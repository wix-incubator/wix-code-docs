    {
      "name": "updateExternalDatabaseConnection",
      "params": [
        {
          "name": "name",
          "type": [
            {
              "nativeType": "string"
            }
          ],
          "doc": "Name of the external database connection.\nAn external database connection may connect to one or more external data collections or tables.\nThese are represented as `connectionName/dataCollectionId`.",
          "required": true
        },
        {
          "name": "options",
          "type": [
            {
              "referenceType": "wix-data-v2.ExternalDatabaseConnections.UpdateExternalDatabaseConnectionOptions"
            }
          ],
          "doc": "Options for updating an external database connection.\n",
          "required": false
        }
      ],
      "requiredFields": [
        "name",
        "options.externalDatabaseConnection.configuration",
        "options.externalDatabaseConnection.endpoint"
      ],
      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-data-v2.ExternalDatabaseConnections.UpdateExternalDatabaseConnectionResponse"
                }
              ]
            }
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "Updates an external database connection.",
        "description": [
          "An external database collection name must be provided in `name`.\nIf an existing external database connection is found with the same name, that connection's details are updated.\nIf no external database connection has that name, the request fails.\n\n> **Note:** After an external database connection is updated, it only contains the values provided in the request. All previous values are lost."
        ],
        "examples": [
          {
            "title": "updateExternalDatabaseConnection example",
            "body": [
              "import { externalDatabaseConnections } from 'wix-data.v2';",
              "  ",
              " async function updateExternalDatabaseConnection(name, options) {",
              "   try {",
              "     const result = await externalDatabaseConnections.updateExternalDatabaseConnection(name, options);",
              "",
              "     return result;",
              "   } catch (error) {",
              "     console.error(error);",
              "     // Handle the error",
              "   }",
              " }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false,
      "customLabels": [
        {
          "id": "maturity-beta"
        }
      ],
      "syntaxName": "updateExternalDatabaseConnection"
    }
