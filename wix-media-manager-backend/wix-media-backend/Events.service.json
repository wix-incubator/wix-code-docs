    [ { "name": "onFileUploaded",
        "labels":
          [ "changed" ],
        "nameParams": [],
        "params":
          [ { "name": "event",
              "type": "wix-media-backend.Events.FileEvent",
              "doc": "The uploaded file information." } ],
